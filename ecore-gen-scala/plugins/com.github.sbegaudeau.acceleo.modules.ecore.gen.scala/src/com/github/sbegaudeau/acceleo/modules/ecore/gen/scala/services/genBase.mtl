[comment encoding = UTF-8 /]
[module genBase('http://www.eclipse.org/emf/2002/GenModel')]

[query public getCopyright(genBase : GenBase, indentation : String) : String
	= invoke('org.eclipse.emf.codegen.ecore.genmodel.GenBase', 'getCopyright(java.lang.String)', Sequence{genBase, indentation}) /]

[query public hasCopyright(genBase : GenBase) : Boolean
	= invoke('org.eclipse.emf.codegen.ecore.genmodel.GenBase', 'hasCopyright()', Sequence{genBase}) /]


